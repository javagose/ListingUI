{
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es2016",
      "dom"
    ],
    "outDir": "../out-tsc/app",
    "target": "es5",
    "module": "es2015",
    "baseUrl": "src",
    "paths": {
            "service/*": [ "app/service/*" ],
            "data-model/*": [ "app/data-model/*" ]
        },
    "types": [] 
  }, 
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
  
}
